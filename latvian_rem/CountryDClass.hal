    end;
    if (HasObjects) then begin    
      EditField(h,v,120,"Realiz. objekts",Normal,Objects,false,ObjSClass);  
    end;
    if (IsEnterprise) then begin
  CheckBox(h,v+=30,0,"R‚d´t t´mekl´",ShowOnWeb);
    end;
    if (HasVATZone) then begin
      v = VATZoneButtonsLabelslat(270,20,170,16);
    end;
  end;
