  TileButton(1,"Teksts");
  TileButton(2,"T´mekºa re®istr‚cija");
  Tile(1,200,true,"1",Math);
  end;
  if (HasLanguages) then begin
  MatrixBegin(4,v=10,-25,-10,INTextRowType,99);
  MatCol(1,24,"Valoda",0,LangCode,false,LangSClass);
  MatCol(1,84,"Teksts",0,Text,false,0);
  EndMatrix;
  end;
  if (HasFreightAdvanced or HasPriceLists or HasObjects) and (StandardHansa==false) then begin
  Tile(2,120,false,"2",FreightCode);
    v = 6;
    if (HasFreightAdvanced) then begin
      EditField(h,v,50,"Pieg‚des kods",Normal,FreightCode,false,WebFreightSClass);  
      v = v + vs;
    end;      
    if (HasPriceLists) then begin    
      EditField(h,v,50,"Cenu lapa",Normal,PLCode,false,PLDefSClass);  
  EditField(h,v+=vs,50,"Atlaiìu matrica",Normal,RebCode,false,RebSClass);
